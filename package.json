{
  "name": "electron-app",
  "productName": "electron-app",
  "private": true,
  "version": "1.4.0",
  "description": "Validation app",
  "main": ".webpack/electron/index.bundle.js",
  "author": {
    "name": "andresmestra"
  },
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"npm run vite:dev\"  \" npm run electron:dev\"",
    "build": "npm run vite:build  && npm run electron:build",
    "package": "npm run build && npm run electron:package",
    "make": "npm run build && npm run electron:make",
    "vite:dev": "vite --mode development",
    "vite:build": "vite build --mode production",
    "electron:dev": "npm run electron:build:dev && npm run electron:start",
    "electron:start": "env-cmd -e dev electron-forge start",
    "electron:build": "env-cmd -e production webpack --config webpack.main.config.js",
    "electron:build:dev": "env-cmd -e dev webpack --config webpack.main.config.js",
    "electron:build:test": "env-cmd -e test webpack --config webpack.main.config.js",
    "electron:package": "env-cmd -e production electron-forge package",
    "electron:make": "env-cmd -e production electron-forge make",
    "electron:rebuild": "electron-rebuild",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "dependencies": {
    "electron-squirrel-startup": "1.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "@ticketplushq/transbank-pos-sdk": "^1.1.3"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.64",
    "@electron-forge/maker-deb": "6.0.0-beta.64",
    "@electron-forge/maker-rpm": "6.0.0-beta.64",
    "@electron-forge/maker-squirrel": "6.0.0-beta.64",
    "@electron-forge/maker-zip": "6.0.0-beta.64",
    "@electron-forge/plugin-auto-unpack-natives": "6.0.0-beta.64",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "@vercel/webpack-asset-relocator-loader": "1.7.2",
    "@vitejs/plugin-react": "1.3.2",
    "concurrently": "7.2.2",
    "dotenv-webpack": "7.1.1",
    "electron": "19.0.8",
    "electron-devtools-installer": "^3.2.0",
    "env-cmd": "10.1.0",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "node-loader": "2.0.0",
    "prettier": "2.7.1",
    "ts-loader": "9.3.1",
    "typescript": "4.7.4",
    "vite": "2.9.13",
    "webpack": "5.73.0",
    "webpack-cli": "4.10.0"
  }
}
